  "browser"       : true,
  "couch"         : false,
  "devel"         : true,
  "dojo"          : false,
  "jquery"        : true,
  "mootools"      : false,
  "node"          : true,
  "nonstandard"   : false,
  "phantom"       : false,
  "prototypejs"   : false,
  "rhino"         : false,
  "worker"        : false,
  "wsh"           : false,
